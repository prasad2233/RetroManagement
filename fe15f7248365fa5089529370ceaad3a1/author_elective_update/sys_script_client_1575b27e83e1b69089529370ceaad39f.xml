        <messages/>
        <name>Check for Duplicate Submission</name>
        <order/>
        <script><![CDATA[function onLoad() {
    // Only check on new records
    if (g_form.isNewRecord()) {
        var sprintId = g_form.getValue('sprint');
        
        if (sprintId) {
            // Check if user already submitted
            var ga = new GlideAjax('RetroManagementAjax');
            ga.addParam('sysparm_name', 'checkDuplicateSubmission');
            ga.addParam('sysparm_sprint_id', sprintId);
            ga.getXMLAnswer(function(answer) {
                if (answer === 'true') {
                    g_form.addErrorMessage('You have already submitted a retrospective for this sprint');
                    g_form.addOption('sprint', '', '-- Select Sprint --');
                    g_form.setValue('sprint', '');
                }
            });
        }
    }
}]]></script>
