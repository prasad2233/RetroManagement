        <name>Mark as Reviewed</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[// UI Action: Mark as Reviewed
// Table: Retrospective Submission

var retroId = g_action.getGlideRecord().getUniqueValue();
var retro = new GlideRecord('x_1865669_retrospe_retrospective_submission');

if (retro.get(retroId)) {
    var status = retro.review_status.toString();
    
    if (status !== 'submitted') {
        gs.addErrorMessage('This retrospective has already been reviewed');
    } else {
        // Set reviewed status
        retro.review_status = 'reviewed';
        retro.reviewed_date = new Date();
        retro.reviewed_by = gs.getUserID();
        
        // Add note
        var notes = retro.work_notes.toString() || '';
        retro.work_notes = notes + '\nMarked as reviewed by ' + gs.getUser().getName();
        
        retro.update();
        
        gs.addInfoMessage('✅ Retrospective marked as reviewed');
        action.setRedirectURL(retro);
    }
} else {
    gs.addErrorMessage('❌ Could not find retrospective');
}]]></script>
