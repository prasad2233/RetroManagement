        <messages/>
        <name>block duplicate</name>
        <order/>
        <script><![CDATA[function onSubmit() {
    // Only check on NEW records
    if (!g_form.isNewRecord()) {
        return true;
    }
    
    var sprintId = g_form.getValue('sprint');
    
    if (!sprintId) {
        return true; // Let other validation handle it
    }
    
    // Check for duplicate
    var ga = new GlideAjax('RetroManagementAjax');
    ga.addParam('sysparm_name', 'checkDuplicateSubmission');
    ga.addParam('sysparm_sprint_id', sprintId);
    
    var answer = ga.getXMLWait();
    
    if (answer === 'true') {
        g_form.addErrorMessage('⛔ Duplicate detected - You have already submitted for this sprint');
        return false; // ✅ BLOCKS SUBMISSION (CLIENT-SIDE)
    }
    
    return true; // ✅ ALLOWS SUBMISSION
}]]></script>
