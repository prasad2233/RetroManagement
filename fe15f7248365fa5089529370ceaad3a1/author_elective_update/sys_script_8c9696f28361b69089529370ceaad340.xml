        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	// Validate start and end dates are set
    if (current.start_date.nil() || current.end_date.nil()) {
        gs.addErrorMessage('Start date and end date are required');
        current.setAbortAction(true);
        return;
    }
    
    // Validate end date is after start date
    var startDate = new GlideDate();
    startDate.setValue(current.start_date);
    
    var endDate = new GlideDate();
    endDate.setValue(current.end_date);
    
    if (endDate.compareTo(startDate) <= 0) {
        gs.addErrorMessage('End date must be after start date');
        current.setAbortAction(true);
        return;
    }


})(current, previous);]]></script>
