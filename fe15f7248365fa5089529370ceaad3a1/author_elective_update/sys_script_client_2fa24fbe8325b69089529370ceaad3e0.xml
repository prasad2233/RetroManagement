        <messages/>
        <name>Check Duplicate Retro on Sprint Selectio</name>
        <order/>
        <script><![CDATA[function onSubmit() {
    // Only check on NEW records
    if (!g_form.isNewRecord()) {
        return true; // Allow updates
    }
    
    var sprintId = g_form.getValue('sprint');
    
    // If no sprint selected, let other validation handle it
    if (!sprintId) {
        return true;
    }
    
    // Synchronous check before submission
    var ga = new GlideAjax('RetroManagementAjax');
    ga.addParam('sysparm_name', 'checkDuplicateSubmission');
    ga.addParam('sysparm_sprint_id', sprintId);
    
    // Use getXMLWait() for synchronous call in onSubmit
    var answer = ga.getXMLWait();
    
    if (answer === 'true') {
        var sprintName = g_form.getDisplayValue('sprint');
        g_form.addErrorMessage('You have already submitted a retrospective for ' + sprintName + '. Submission blocked.');
        return false; // Block form submission
    }
    
    return true; // Allow submission
}]]></script>
