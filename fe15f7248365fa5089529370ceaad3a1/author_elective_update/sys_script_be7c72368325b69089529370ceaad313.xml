        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

    // Require sprint + submitted_by
    if (!current.sprint || !current.submitted_by)
        return;

    var gr = new GlideRecord('x_1865669_retrospe_retrospective_submission');
    gr.addQuery('sprint', current.sprint);
    gr.addQuery('submitted_by', current.submitted_by);
    // Ignore the same record on update (only needed if you also run on update)
    if (!current.isNewRecord())
        gr.addQuery('sys_id', '!=', current.sys_id);
    gr.setLimit(1);
    gr.query();

    if (gr.hasNext()) {
        gs.addErrorMessage('You can submit only one retrospective per sprint.');
        current.setAbortAction(true);
    }

})(current, previous);]]></script>
