        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
    
    // Only validate if sprint or submitted_by changed (for updates)
    if (!current.isNewRecord() && 
        !current.sprint.changes() && 
        !current.submitted_by.changes()) {
        return; // No need to check if these fields didn't change
    }
    
    // Validate that required fields are present
    if (current.sprint.nil() || current.submitted_by.nil()) {
        return; // Let other validation handle missing required fields
    }
    
    // Check if this employee already submitted for this sprint
    var retroGR = new GlideRecord('x_retro_mgmt_retro_submission');
    retroGR.addQuery('sprint', current.sprint);
    retroGR.addQuery('submitted_by', current.submitted_by);
    retroGR.addQuery('sys_id', '!=', current.sys_id); // Exclude current record
    retroGR.query();
    
    if (retroGR.hasNext()) {
        // Log the attempt for auditing
        gs.error('Duplicate retro submission attempt - User: ' + current.submitted_by.getDisplayValue() + 
                 ', Sprint: ' + current.sprint.getDisplayValue());
        
        // Show error to user
        gs.addErrorMessage('You have already submitted a retrospective for this sprint. Only one submission per employee per sprint is allowed.');
        
        // Abort the transaction
        current.setAbortAction(true);
        return;
    }
    
})(current, previous);]]></script>
