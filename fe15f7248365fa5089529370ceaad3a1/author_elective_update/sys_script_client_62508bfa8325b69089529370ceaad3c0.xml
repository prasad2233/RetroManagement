        <messages/>
        <name>Prevent Duplicate Retro on Submit</name>
        <order/>
        <script><![CDATA[function onSubmit() {
   //Type appropriate comment here, and begin script 
       if (!g_form.isNewRecord()) {
        return true; // Allow updates
    }
    
    var sprintId = g_form.getValue('sprint');
    
    if (!sprintId) {
        return true; // Let other validation handle empty sprint
    }
    
    // Synchronous check using GlideRecord (client-side limitation workaround)
    // Note: This makes a synchronous call which may slow form submission
    var ga = new GlideAjax('RetroManagementAjax');
    ga.addParam('sysparm_name', 'checkDuplicateSubmission');
    ga.addParam('sysparm_sprint_id', sprintId);
    
    // Use synchronous call for onSubmit
    var answer = ga.getXMLWait();
    
    if (answer === 'true') {
        g_form.addErrorMessage('You have already submitted a retrospective for this sprint. Submission blocked.');
        return false; // Block submission
    }
    
    return true; // Allow submission   
}]]></script>
