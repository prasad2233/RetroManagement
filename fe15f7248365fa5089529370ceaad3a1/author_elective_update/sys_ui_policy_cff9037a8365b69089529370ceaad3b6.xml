        <script_false><![CDATA[function onCondition() {

}]]></script_false>
        <script_true><![CDATA[function onCondition() {
    if (g_form.isNewRecord()) {
        var sprintId = g_form.getValue('sprint');
        var userId = g_form.getValue('submitted_by');

        if (sprintId && userId) {
            // Check for duplicate
            var ga = new GlideAjax('RetroManagementAjax');
            ga.addParam('sysparm_name', 'checkDuplicateSubmission');
            ga.addParam('sysparm_sprint_id', sprintId);

            ga.getXMLAnswer(function(answer) {
                if (answer === 'true') {
                    g_form.addErrorMessage('You have already submitted for this sprint');
                    g_form.removeSubmitButton();
                }
            });
        }
    }

}]]></script_true>
