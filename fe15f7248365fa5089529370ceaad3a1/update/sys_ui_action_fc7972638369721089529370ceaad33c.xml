        <name>Mark retro as Reviewd </name>
        <onclick/>
        <order>310</order>
        <script><![CDATA[(function () {

    // Only allow for valid (or submitted+valid if you used the relaxed condition)
    if (current.status != 'valid' && current.status != 'submitted') {
        gs.addErrorMessage('This action is only available for submitted or valid retros.');
        action.setRedirectURL(current);
        return;
    }

    // Treat "completed" as reviewed
    current.status = 'reviewed ';
    current.reviewed_by = gs.getUserID();
    current.reviewed_date = new GlideDateTime();

    current.update();

    gs.addInfoMessage('Retrospective marked as completed (reviewed).');
    action.setRedirectURL(current);

})();]]></script>
