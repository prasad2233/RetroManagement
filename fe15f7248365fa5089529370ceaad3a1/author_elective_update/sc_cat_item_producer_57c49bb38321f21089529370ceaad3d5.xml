        <script><![CDATA[(function() {
    try {
        // Create new retrospective submission record
        var gr = new GlideRecord('x_1865669_retrospe_retrospective_submission');
        gr.initialize();
        
        // Map variables to fields
        gr.sprint = producer.number;
        gr.start = producer.start_items;
        gr.stop = producer.stop_items;
        gr.continue_doing = producer.continue_items;
        
        // Set submitted by and date automatically
        gr.submitted_by = gs.getUserID();
        gr.submitted_date = gs.nowDateTime();
        
        // Insert the record
        var sysId = gr.insert();
        
        if (sysId) {
            // Set success message
            gs.addInfoMessage('Thank you! Your retrospective feedback has been submitted successfully.');
            
            // Redirect to the retrospectives list page
            action.setRedirectURL('/esc?id=my_retrospectives');
        } else {
            gs.addErrorMessage('Failed to submit retrospective. Please try again.');
        }
        
    } catch (e) {
        gs.error('Error submitting retrospective: ' + e.getMessage());
        gs.addErrorMessage('An error occurred while submitting. Please contact your administrator.');
    }
})();]]></script>
