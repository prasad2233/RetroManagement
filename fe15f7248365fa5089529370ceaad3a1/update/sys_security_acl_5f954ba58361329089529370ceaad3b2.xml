        <decision_type>allow</decision_type>
        <description>Allow write for records in x_1865669_retrospe_retrospective_submission, for users with role x_1865669_retrospe.employee, and if the ACL script returns true.</description>
        <local_or_existing>Local</local_or_existing>
        <name>x_1865669_retrospe_retrospective_submission</name>
        <operation display_value="write">write</operation>
        <script><![CDATA[answer = (function executeRule(current, previous) {
    
	
    // Must be the submitter
    if (current.submitted_by == gs.getUserID()) {
           return false; // OK to edit
    }
	
	if (current.status == 'submitted') {
           return true; // OK to edit
       }
    
    
    // Cannot edit after manager reviewed
    return false;
    
})(current, previous);
]]></script>
