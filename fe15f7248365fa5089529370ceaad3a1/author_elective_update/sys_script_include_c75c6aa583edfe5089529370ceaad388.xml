        <api_name>x_1865669_retrospe.RetroTeamHelper</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description/>
        <mobile_callable>false</mobile_callable>
        <name>RetroTeamHelper</name>
        <sandbox_callable>true</sandbox_callable>
        <script><![CDATA[var RetroTeamHelper = Class.create();
RetroTeamHelper.prototype = Object.extendsObject(AbstractAjaxProcessor, {
    
    getUserTeam: function() {
        var userId = this.getParameter('sysparm_user_id');
        
        if (!userId) {
            return '';
        }
        
        // Query team_member table to find user's team
        var teamMember = new GlideRecord('x_1865669_retrospe_team_member');
		gs.info("team name is "+userId);
        teamMember.addQuery('user', userId);
        teamMember.addQuery('active', true);
        teamMember.query();
		
        
        if (teamMember.next()) {
            // Return the team sys_id
            return teamMember.team.toString();
        }
        
        return '';
    },
    
    type: 'RetroTeamHelper'
});]]></script>
