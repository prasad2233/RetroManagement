        <description/>
        <mobile_callable>false</mobile_callable>
        <name>RetroManagementAjax</name>
        <sandbox_callable>true</sandbox_callable>
        <script><![CDATA[var RetroManagementAjax = Class.create();
RetroManagementAjax.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
    
    checkDuplicateSubmission: function() {
        var sprintId = this.getParameter('sysparm_sprint_id');
        var userId = gs.getUserID();
        
        // Query for existing submission
        var retroGR = new GlideRecord('x_retro_mgmt_retro_submission');
        retroGR.addQuery('sprint', sprintId);
        retroGR.addQuery('submitted_by', userId);
        retroGR.query();
        
        // Return 'true' if duplicate exists, 'false' otherwise
        return retroGR.hasNext().toString();
    },
    
    type: 'RetroManagementAjax'
});
]]></script>
