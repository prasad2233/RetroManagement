        <applies_to/>
        <condition/>
        <decision_type>allow</decision_type>
        <description>Allow create for records in x_1865669_retrospe_retrospective_submission, for users with role x_1865669_retrospe.employee, and if the ACL script returns true.</description>
        <local_or_existing>Local</local_or_existing>
        <name>x_1865669_retrospe_retrospective_submission</name>
        <operation display_value="create">create</operation>
        <script><![CDATA[// Allow employees to submit only when sprint is ready
(function executeRule(current, previous) {

    var sprint = current.sprint.getRefRecord();
	gs.info("sprint is "+sprint.state)
    if (sprint.state == 'ready_for_submission') {
        return true;
    }
    
    return false;
})(current, previous);

]]></script>
