        <name>Mark as Not Needed</name>
        <onclick/>
        <order>200</order>
        <script><![CDATA[(function () {

    // Allow only from submitted (or submitted+valid if you used the relaxed condition)
    if (current.status != 'submitted' && current.status != 'valid') {
        gs.addErrorMessage('Only submitted or valid retros can be marked as not needed.');
        action.setRedirectURL(current);
        return;
    }

    current.status = 'not_needed';
    current.reviewed_by = gs.getUserID();
    current.reviewed_date = new GlideDateTime();

    current.update();

    gs.addInfoMessage('Retrospective marked as Not Needed.');
    action.setRedirectURL(current);

})();]]></script>
